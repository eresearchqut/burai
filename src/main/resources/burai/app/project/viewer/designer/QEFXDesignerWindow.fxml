<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>

<Group fx:id="baseGroup" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane styleClass="designerwin-pane">
         <children>
            <Pane fx:id="mainPane" prefHeight="400.0" prefWidth="400.0" />
            <Button fx:id="scaleButton" mnemonicParsing="false" prefHeight="35.0" prefWidth="35.0" text="\@" StackPane.alignment="TOP_RIGHT">
               <StackPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </StackPane.margin></Button>
            <Label fx:id="headLabel" style="-fx-font-family: monospace; -fx-font-weight: bold; -fx-font-size: 1.333333em;" text="origin" StackPane.alignment="TOP_CENTER">
               <StackPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </StackPane.margin>
            </Label>
         </children>
      </StackPane>
   </children>
</Group>
