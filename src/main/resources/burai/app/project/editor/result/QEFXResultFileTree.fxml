                        <Insets left="8.0" />
                     </GridPane.margin>
                  </Button>
                  <Label text="Stop Calculation" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" prefWidth="50.0" />
